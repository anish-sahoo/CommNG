{
  "name": "server",
  "type": "module",
  "version": "0.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "dev": "npx dotenv -e .env -- tsx watch --clear-screen=false src/index.ts",
    "test": "vitest",
    "db:generate": "npx dotenv -e .env -- drizzle-kit generate --config=drizzle.config.ts  --name=migration",
    "db:migrate": "npx dotenv -e .env -- drizzle-kit migrate --config=drizzle.config.ts",
    "db:studio": "npx dotenv -e .env -- drizzle-kit studio --config=drizzle.config.ts",
    "db:seed": "npx dotenv -e .env -- tsx src/utils/seed.ts",
    "format": "npx @biomejs/biome format --write",
    "lint": "npx @biomejs/biome check",
    "lintfix": "npx @biomejs/biome check --write"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "@types/pg": "^8.15.5",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.5",
    "pino-pretty": "^13.1.1",
    "trpc-ui": "^1.0.15",
    "tsx": "^4.20.6",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "dompurify": "^3.2.4",
    "esbuild": "^0.25.0"
  },
  "dependencies": {
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.44.6",
    "express": "^5.1.0",
    "p-limit": "^7.1.1",
    "pg": "^8.16.3",
    "pino": "^10.0.0",
    "redis": "^5.8.3"
  }
}
